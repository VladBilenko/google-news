{
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "variable-name": [
      true,
      "ban-keywords",
      "allow-leading-underscore"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "no-any": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "space-before-function-paren": [true, {"anonymous": "always", "named": "never"}],
    "interface-name": [
      true,
      "always-prefix"
    ],
    "array-type": [
      true,
      "generic"
    ],
    "object-literal-sort-keys": false,
    "no-trailing-whitespace": false,
    "trailing-comma": [
      false,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    ],
    "no-console": {
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  },
  "defaultSeverity": "error"
}
